{
	"info": {
		"_postman_id": "a8c4bfc1-f9fe-42a0-b41e-470b9c8490ef",
		"name": "Magic Eden",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "NFT",
			"item": [
				{
					"name": "Get info",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var data = JSON.parse(responseBody);\r",
									"postman.setEnvironmentVariable(\"mgNFTOwnerAddress\", data.owner)\r",
									"postman.setEnvironmentVariable(\"mgNFTCollectionSymbolAddress\", data.collection)\r",
									"postman.setEnvironmentVariable(\"mgNFTName\", data.name)\r",
									"postman.setEnvironmentVariable(\"mgNFTWebsiteURL\", data.externalUrl)\r",
									"postman.setEnvironmentVariable(\"mgNFTCreatorAddress\", data.properties.creators[0].address)\r",
									"postman.setEnvironmentVariable(\"mgNFTCreatorFee\", data.properties.creators[0].share)"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{mgApiUrl}}/tokens/{{mgNFTMintAddress}}",
							"host": [
								"{{mgApiUrl}}"
							],
							"path": [
								"tokens",
								"{{mgNFTMintAddress}}"
							]
						}
					},
					"response": []
				},
				{
					"name": "Change price (Auction)",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{mgApiUrl}}/instructions/sell_change_price?seller={{mgWalletAddress}}&auctionHouseAddress&tokenMint={{mgNFTMintAddress}}&tokenAccount={{mgWalletAddress}}&price={{mgNFTAuctionCurrentSellPrice}}&newPrice={{mgNFTAuctionNewSellPrice}}",
							"host": [
								"{{mgApiUrl}}"
							],
							"path": [
								"instructions",
								"sell_change_price"
							],
							"query": [
								{
									"key": "seller",
									"value": "{{mgWalletAddress}}"
								},
								{
									"key": "auctionHouseAddress",
									"value": null
								},
								{
									"key": "tokenMint",
									"value": "{{mgNFTMintAddress}}"
								},
								{
									"key": "tokenAccount",
									"value": "{{mgWalletAddress}}"
								},
								{
									"key": "price",
									"value": "{{mgNFTAuctionCurrentSellPrice}}"
								},
								{
									"key": "newPrice",
									"value": "{{mgNFTAuctionNewSellPrice}}"
								}
							]
						}
					},
					"response": []
				}
			]
		},
